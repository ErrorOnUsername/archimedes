#import "core::io"

decl some_proc : (param: u16) -> f32
{
	let ret: f32 = param as f32;
	// Implicitly gets type usize
	let other_thing := 20;

	ret += 10.3e5;
	ret *= other_thing - 12;

	other_thing = 6;

	ret /= other_thing;

	return ret;
}

decl CT_RESULT := #exec some_proc(10);

// Implicitly returns nothing (i.e. decl main : () -> nothing { })
decl main : ()
{
	let param: u16 = 10;
	let float_val := some_proc(param);

	println("The value we got from 'some_proc' with param: %d is %f", param, float_val);
	println("Using the same param at compile time, we got: %f", CT_RESULT);
}
